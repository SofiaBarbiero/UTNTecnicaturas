                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        E.ape_est + ' ' + E.nom_est AS 'Estudiante', STR(AVG(EM1.calificacion), 4, 2) AS 'Calificacion', MAX(EM1.calificacion) AS 'NotaM', MIN(EM1.calificacion) AS 'Notam'
FROM            ESTUDIANTES AS E INNER JOIN
                         ESTUDIANTES_MATERIAS AS EM ON EM.legajo_est = E.legajo_est INNER JOIN
                         MATERIAS AS M1 ON M1.id_materia = EM.id_materia INNER JOIN
                         EXAMEN_MATERIAS AS EM1 ON EM1.id_materia = M1.id_materia INNER JOIN
                         TECNICATURAS_MATERIAS AS TM ON TM.id_materia = M1.id_materia INNER JOIN
                         TECNICATURAS AS T ON T.id_tecnicatura = TM.id_tecnicatura INNER JOIN
                         EXAMENES AS EX ON EX.id_examen = EM1.id_examen
WHERE        (T.id_tecnicatura = 1) AND (M1.id_materia IN (1, 7, 8)) AND (YEAR(EX.fecha_examen) BETWEEN @DesdeAño AND @HastaAño)
GROUP BY E.ape_est + ' ' + E.nom_est, E.legajo_est
HAVING        (AVG(EM1.calificacion) &gt;
                             (SELECT        AVG(calificacion) AS Expr1
                               FROM            EXAMEN_MATERIAS AS exm))
ORDER BY 'Estudiante'</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="DesdeAño" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="Decimal" Direction="Input" ParameterName="@DesdeAño" Precision="0" Scale="0" Size="0" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
                      <Parameter AllowDbNull="false" AutogeneratedName="HastaAño" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="Decimal" Direction="Input" ParameterName="@HastaAño" Precision="0" Scale="0" Size="0" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
