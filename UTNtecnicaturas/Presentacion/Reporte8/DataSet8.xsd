                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        D.ape_doc + ' ' + D.nom_doc AS 'Docente', N.nacion AS 'Nacionalidad', EC.estado_civil AS 'EstadoCivil', CAST(AVG(EM.calificacion) AS decimal(4, 2)) AS 'CalificacionPromedio'
FROM            DOCENTES AS D INNER JOIN
                         NACIONALIDADES AS N ON N.id_nacionalidad = D.id_nacionalidad INNER JOIN
                         ESTADOS_CIVILES AS EC ON EC.id_estado_civil = D.id_estado_civil INNER JOIN
                         DOCENTES_MATERIAS AS DM ON DM.legajo_doc = D.legajo_doc INNER JOIN
                         MATERIAS AS M ON M.id_materia = DM.id_materia INNER JOIN
                         EXAMEN_MATERIAS AS EM ON EM.id_materia = M.id_materia INNER JOIN
                         EXAMEN_MATERIAS AS EM2 ON EM2.id_materia = M.id_materia INNER JOIN
                         EXAMENES AS E ON E.id_examen = EM.id_examen INNER JOIN
                         TECNICATURAS_MATERIAS AS TM ON TM.id_materia = M.id_materia INNER JOIN
                         TECNICATURAS AS T ON T.id_tecnicatura = TM.id_tecnicatura
WHERE        (DATEDIFF(YEAR, E.fecha_examen, GETDATE()) &lt; @anios)
GROUP BY D.ape_doc + ' ' + D.nom_doc, N.nacion, EC.estado_civil, D.legajo_doc, DM.legajo_doc
HAVING        (@materia &lt;
                             (SELECT        COUNT(*) AS Expr1
                               FROM            DOCENTES_MATERIAS AS DM2
                               WHERE        (legajo_doc = DM.legajo_doc))) AND (AVG(EM.calificacion) &gt;
                             (SELECT        AVG(calificacion) AS Expr1
                               FROM            EXAMEN_MATERIAS AS EM3))
ORDER BY D.legajo_doc</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="anios" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="Decimal" Direction="Input" ParameterName="@anios" Precision="0" Scale="0" Size="0" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
                      <Parameter AllowDbNull="false" AutogeneratedName="materia" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="AnsiString" Direction="Input" ParameterName="@materia" Precision="0" Scale="0" Size="1024" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
